<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <tabs>
    <Tab text="List of students">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="Tab1StudentTable" layoutX="1.0" layoutY="-1.0" prefHeight="370.0" prefWidth="600.0">
                    <columns>
                      <TableColumn fx:id="Tab1StudentFirstName" prefWidth="75.0" text="First name" />
                      <TableColumn fx:id="Tab1StudentLastName" prefWidth="75.0" text="Last name" />
                    </columns>
                  </TableView>
               </children></AnchorPane>
      </content>
    </Tab>
    <Tab text="Show student &amp; Course info">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <BorderPane layoutX="-3.0" layoutY="-2.0" prefHeight="374.0" prefWidth="607.0">
                     <top>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                           <center>
                              <TableView fx:id="StudentInfoTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                <columns>
                                  <TableColumn fx:id="StudentFirstNameColumn" prefWidth="75.0" text="First name" />
                                    <TableColumn fx:id="StudentLastNameColumn" prefWidth="75.0" text="Last name" />
                                    <TableColumn fx:id="StudentAvgGradeColumn" prefWidth="75.0" text="Average Grade" />
                                    <TableColumn fx:id="StudentCourseColumn" prefWidth="75.0" text="Course" />
                                    <TableColumn fx:id="StudentGradeColumn" prefWidth="75.0" text="Grade" />
                                    <TableColumn fx:id="StudentSemesterColumn" prefWidth="75.0" text="Semester" />
                                    <TableColumn fx:id="StudentTeacherColumn" prefWidth="75.0" text="Teacher" />
                                </columns>
                              </TableView>
                           </center>
                           <top>
                              <FlowPane prefHeight="41.0" prefWidth="607.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Choose Student:">
                                       <FlowPane.margin>
                                          <Insets left="10.0" right="10.0" top="10.0" />
                                       </FlowPane.margin>
                                    </Label>
                                    <ComboBox fx:id="StudentComboBox" prefWidth="150.0">
                                       <FlowPane.margin>
                                          <Insets right="10.0" top="10.0" />
                                       </FlowPane.margin>
                                    </ComboBox>
                                    <Button fx:id="PrintStudentButton" mnemonicParsing="false" onAction="#PrintStudentToTable" text="Print student!">
                                       <FlowPane.margin>
                                          <Insets top="10.0" />
                                       </FlowPane.margin>
                                    </Button>
                                 </children>
                              </FlowPane>
                           </top>
                        </BorderPane>
                     </top>
                     <bottom>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                           <center>
                              <TableView fx:id="CourseInfoTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                <columns>
                                  <TableColumn fx:id="CourseNameColumn" prefWidth="75.0" text="Course name" />
                                  <TableColumn fx:id="CourseSemesterColumn" prefWidth="75.0" text="Semester" />
                                    <TableColumn fx:id="CourseTeacherColumn" prefWidth="75.0" text="Teacher" />
                                    <TableColumn fx:id="CourseAvgGradeColumn" prefWidth="75.0" text="Avg. Grade" />
                                </columns>
                              </TableView>
                           </center>
                           <top>
                              <FlowPane prefHeight="35.0" prefWidth="607.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Choose Course:">
                                       <FlowPane.margin>
                                          <Insets left="10.0" right="10.0" top="5.0" />
                                       </FlowPane.margin></Label>
                                    <ComboBox fx:id="ChooseCourseComboBox" prefWidth="150.0">
                                       <FlowPane.margin>
                                          <Insets right="10.0" top="5.0" />
                                       </FlowPane.margin></ComboBox>
                                    <Button fx:id="PrintCourseButton" mnemonicParsing="false" onAction="#PrintCourse" text="Print course!">
                                       <FlowPane.margin>
                                          <Insets top="5.0" />
                                       </FlowPane.margin></Button>
                                 </children>
                              </FlowPane>
                           </top>
                        </BorderPane>
                     </bottom>
                  </BorderPane>
               </children></AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
